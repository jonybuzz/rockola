
<head>
    <% include ./partials/head %>
</head>
<body class="body-image">
    <div class="container">
        <h1 class='logo'>Rockola</h1>
        <br>
        <div class="row">
            <div class="col-md-4">
                <form class="form-inline">
                    <div class="form-group">
                        <label for="link-tema">Url:</label>
                        <input type="text" class="form-control" id="link-tema" placeholder="Ingresá la url de tu tema!!"/>
                    </div>
                    <button class="btn btn-primary" id="js-boton-enviar">Agregar!</button>
                </form>
            </div>
            <div class="col-md-8">
                <table class="table lista-reproduccion">
                    <thead>
                        <tr>
                            <th></th>
                            <th>
                                <h4>Título del Tema</h4>
                            </th>
                        </tr>
                    </thead>
                    <tbody id="body-lista-reproduccion">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <script src="/js/lib/jquery-2.2.3.min.js" type="text/javascript"></script>
    <script src="/js/lib/jsrender.min.js" type="text/javascript"></script>

    <script src="js/app/rockola.js"></script>
    <script src="js/app/service/service.js"></script>
    <script src="js/app/service/tema/tema.js"></script>
    <script src="js/app/ui/ui.js"></script>
    <script src="js/app/ui/cliente/cliente.js"></script>    
</body>

<script id="bodyListaTemplate" type="text/html" >
    <tr>
        <td>
            <img src="{{:thumbnail}}">
        </td>
        <td>
            <h4>{{:titulo}}</h4>
        </td>
    </tr>
</script>
